@font-face {
  font-family: 'online_exhibition';
  src:  url('../fonts/online_exhibition.eot?y0r3bk');
  src:  url('../fonts/online_exhibition.eot?y0r3bk#iefix') format('embedded-opentype'),
    url('../fonts/online_exhibition.ttf?y0r3bk') format('truetype'),
    url('../fonts/online_exhibition.woff?y0r3bk') format('woff'),
    url('../fonts/online_exhibition.svg?y0r3bk#online_exhibition') format('svg');
  font-weight: normal;
  font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'online_exhibition' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-arrow_lift:before {
  content: "\e900";
}
.icon-zoom-in:before {
  content: "\e987";
}
.icon-zoom-out:before {
  content: "\e988";
}
